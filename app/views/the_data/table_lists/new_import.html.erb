<div class="three wide column">
  <%= render 'the_data/base/nav' %>
</div>

<div class="thirteen wide column">

  <div class="ui breadcrumb">
    <%= link_to 'Back', data_list_table_lists_path(@data_list), class: 'section' %>
    <div class="divider"> / </div>
    <div class="active section">New</div>
  </div>

  <div class="ui segment">
    <%= default_form_with model: @table_list, url: import_data_list_table_lists_path(@data_list) do |f| %>
      <%= f.file_field :file, label: 'File' %>
      <%= f.submit %>
    <% end %>
  </div>

</div>
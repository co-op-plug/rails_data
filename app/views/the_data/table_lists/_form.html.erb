<%= default_form_for [@data_list, @table_list] do |f| %>
  <% @data_list.parameters.each do |k, v| %>
    <div class="inline fields">
      <label class="six wide field"><%= k.to_s.titleize %>:</label>
      <div class="six wide field">
        <%= text_field_tag :"table_list[parameters][#{k}]", nil, type: TableList::TYPE[v.to_sym][:input] %>
      </div>
    </div>
  <% end %>
  <%= f.submit %>
<% end %>

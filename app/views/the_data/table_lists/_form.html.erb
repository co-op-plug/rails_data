<%= default_form_for [@data_list, @table_list] do |f| %>
  <% @data_list.parameters.each do |k, v| %>
    <div class="inline field">
      <label class="six wide field"><%= k.to_s.titleize %>:</label>
      <%= text_field_tag :"table_list[parameters][#{k}]", nil, class: 'nine wide field', type: Conversion::TYPE[v.to_sym][:input] %>
    </div>
  <% end %>
  <%= f.submit %>
<% end %>

<div class="three wide column">
  <%= render 'admin/base/nav_users' %>
</div>

<div class="thirteen wide column">

  <div class="ui breadcrumb">
    <%= link_to 'Back', data_lists_path, class: 'section' %>
    <div class="divider"> / </div>
    <div class="active section">Edit</div>
  </div>

  <div class="ui segment">
    <%= default_form_for [@data_list, @table_list] do |f| %>
      <% @table_list.parameters.each do |k, v| %>
        <%= text_field_tag :"table_list[parameters][#{k}]", v, label: k.to_s.titleize %>
      <% end %>
      <%= f.submit %>
    <% end %>
  </div>

</div>
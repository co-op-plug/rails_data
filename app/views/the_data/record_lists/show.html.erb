<div>

  <ul class="ui segment breadcrumb">
    <li class="section"><%= link_to 'Data List', data_lists_path %></li>
    <li class="divider">/</li>
    <li class="section"><%= link_to 'Table List', data_list_record_lists_path(@data_list) %></li>
  </ul>

  <div class="ui top attached menu borderless">
    <div class="item"><strong>Preview</strong></div>
  </div>

  <div class="ui top attached menu borderless">
    <div class="item"><strong>TableID:<%= @record_list.id %></strong></div>
    <div class="right menu">
      <%= link_to 'download Csv', data_list_record_list_path(@data_list, @record_list, format: 'csv'), class: 'item ui button' %>
      <%= link_to 'download Xlsx', data_list_record_list_path(@data_list, @record_list, format: 'xlsx'), class: 'item ui blue button' %>
    </div>
  </div>

  <%= render 'table' %>

</div>

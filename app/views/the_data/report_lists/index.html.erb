<div class="page-header">
  <h1>Report lists</h1>
</div>


<table class="table table-striped">
  <thead>
    <tr>
      <th>ID</th>
      <th>File Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @report_lists.each do |report_list| %>
      <tr>
        <td><%= report_list.id %></td>
        <td><%= report_list.title %></td>
        <td><%= link_to 'table list', report_list_table_lists_path(report_list) %></td>
        <td><%= link_to 'download', report_list_path(report_list) %></td>
        <td><%= link_to 'destroy', report_list_path(report_list), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

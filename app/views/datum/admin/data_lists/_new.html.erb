<div class="ui modal" id="modal">
  <i class="close icon"></i>
  <div class="ui header">
    <%= t('.new') %>
  </div>

  <div class="content">
    <%= form_with model: @data_list, scope: :data_list, url: data_lists_path do |f| %>
      <%= render partial: 'form', locals: { f: f } %>
      <%= f.submit %>
    <% end %>
  </div>
</div>
